{
  "name": "govote",
  "description": "govote",
  "license": "MIT",
  "author": "HsuTing <hsuting0106@gmail.com>",
  "version": "1.0.0",
  "scripts": {
    "dev": "node --inspect ./src/server",
    "heroku-postbuild": "patch-package && yarn relay && yarn prod",
    "postinstall": "patch-package",
    "prod": "next build ./src",
    "relay": "relay-compiler --src ./src --exclude --schema ./schema.graphql --exclude=[**/.next/**]",
    "start": "NODE_ENV=production node ./src/server",
    "update-cache": "babel-node ./src/bin/updateCache.js"
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@zeit/next-less": "^1.0.1",
    "antd": "^3.10.7",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-relay": "^1.7.0",
    "chalk": "^2.4.1",
    "graphql": "^0.13.0",
    "graphql-tools": "^4.0.3",
    "isomorphic-unfetch": "^3.0.0",
    "koa": "^2.5.2",
    "koa-better-http-proxy": "^0.2.4",
    "koa-graphql": "^0.8.0",
    "koa-router": "^7.4.0",
    "less": "^3.8.1",
    "mapbox-gl": "^0.51.0",
    "memoize-one": "^4.0.3",
    "moment": "^2.22.2",
    "next": "^7.0.2",
    "output-file-sync": "^2.0.1",
    "patch-package": "^5.1.1",
    "postinstall-prepare": "^1.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-relay": "^1.7.0",
    "react-relay-network-modern": "^2.4.0",
    "react-relay-network-modern-ssr": "^1.2.1",
    "relay-compiler": "^1.7.0",
    "style-loader": "^0.23.1",
    "svg-react-loader": "^0.4.6",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/node": "^7.0.0",
    "husky": "^1.1.4",
    "lint-staged": "^8.0.5",
    "prettier": "^1.15.2",
    "prettier-package-json": "^2.0.1"
  },
  "keywords": ["govote"],
  "engines": {
    "node": ">= 10.2.1",
    "npm": " >= 6.1.0",
    "yarn": ">= 1.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
